{
  "name": "iot-smart-parking-system",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "mobile": "pnpm --filter mobile dev",
    "server": "pnpm --filter server dev",
    "schemas": "pnpm --filter @iot-smart-parking-system/shared-schemas dev",
    "dev": "pnpm --parallel --filter mobile --filter server --filter @iot-smart-parking-system/shared-schemas dev",
    "dev:server": "pnpm --parallel --filter @iot-smart-parking-system/shared-schemas --filter server dev",
    "build:schemas": "pnpm --filter @iot-smart-parking-system/shared-schemas build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "keywords": [],
  "author": "Leon",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1"
  }
}
